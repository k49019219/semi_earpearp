package my.day01;

public class HelloTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println("안녕하세요? 자바를 처음 합니다.");
		System.out.println("");
		
		for(int i=1; i<10; i++) {
			for(int j=10; j<1; j--) {
				System.out.println("*");
			}
			System.out.println(i);
		}
		
	}

}
